USE GRAPH ldbc_snb
SET syntax_version="v2"

CREATE OR REPLACE DISTRIBUTED QUERY orderby_largek(){
TYPEDEF TUPLE<UINT forumID> RESULT;
HeapAccum<RESULT>(forumID ASC) @@result;

forums = SELECT f
        FROM Forum:f
        Accum @@result+=RESULT(f.id)
        ORDER BY f.id;

PRINT @@result.size();


}