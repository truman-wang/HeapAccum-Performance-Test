USE GRAPH lsqb
SET syntax_version="v2"

CREATE OR REPLACE DISTRIBUTED QUERY pop_smallk(){



TYPEDEF TUPLE<UINT forumID> RESULT;
HeapAccum<RESULT>(20, forumID ASC) @@result;

forums = SELECT f
        FROM Forum:f
        Accum @@result+=RESULT(f.id);
INT k = @@result.size();     
WHILE k != 0 DO
@@result.pop();
k=k-1;
END;

PRINT @@result;


}