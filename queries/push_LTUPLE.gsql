USE GRAPH ldbc_snb
SET syntax_version="v2"

CREATE OR REPLACE DISTRIBUTED QUERY push_LTUPLE(INT k){
TYPEDEF TUPLE<UINT forumID, STRING forumTitle, DATETIME forumCreationDate> RESULT;
HeapAccum<RESULT>(k,forumCreationDate ASC, forumTitle ASC, forumID ASC) @@result;

forums = SELECT f
        FROM Forum:f
        Accum @@result+=RESULT(f.id, f.title, f.creationDate);

PRINT @@result.size();


}