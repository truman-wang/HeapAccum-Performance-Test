USE GRAPH lsqb
SET syntax_version="v2"

CREATE OR REPLACE DISTRIBUTED QUERY resize_largek(){



TYPEDEF TUPLE<UINT forumID> RESULT;
HeapAccum<RESULT>(forumID ASC) @@result;

forums = SELECT f
        FROM Forum:f
        Accum @@result+=RESULT(f.id);
 INT k = @@result.size();    
WHILE k != 0 DO
@@result.resize(k);
k=k-1;
END;

PRINT @@result;


}